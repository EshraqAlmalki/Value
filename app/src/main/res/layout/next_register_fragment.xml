<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".authentication.register.NextRegisterFragment">

    <ImageView
        android:id="@+id/done_iv"
        android:layout_width="81dp"
        android:layout_height="55dp"
        android:layout_marginTop="628dp"
        android:layout_marginEnd="28dp"
        android:src="@drawable/ic_baseline_done_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout5"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
        android:layout_width="169dp"
        android:layout_height="72dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout3">

        <AutoCompleteTextView
            android:id="@+id/gender_info"
            android:layout_width="151dp"
            android:layout_height="49dp"
            android:layout_weight="1"
            android:hint="@string/register_gender"
            android:inputType="none"
            tools:ignore="TouchTargetSizeCheck,DuplicateClickableBoundsCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
        android:layout_width="169dp"
        android:layout_height="72dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="44dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <AutoCompleteTextView
            android:id="@+id/weight_info"
            android:layout_width="151dp"
            android:layout_height="49dp"
            android:layout_weight="1"
            android:hint="@string/register_weight"
            android:inputType="none"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout4"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
        android:layout_width="169dp"
        android:layout_height="72dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="30dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <AutoCompleteTextView
            android:id="@+id/height_info"
            android:layout_width="151dp"
            android:layout_height="49dp"
            android:layout_weight="1"
            android:hint="@string/register_height"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout2"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
        android:layout_width="169dp"
        android:layout_height="72dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="30dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout3">

        <AutoCompleteTextView
            android:id="@+id/active_level_info"
            android:layout_width="151dp"
            android:layout_height="49dp"
            android:layout_weight="1"
            android:hint="@string/register_active_level"
            android:text=""
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout3"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
        android:layout_width="329dp"
        android:layout_height="61dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="30dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">


        <AutoCompleteTextView
            android:id="@+id/weight_goal_info"
            android:layout_width="315dp"
            android:layout_height="49dp"
            android:layout_weight="1"
            android:hint="@string/register_goal" />
    </com.google.android.material.textfield.TextInputLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="421dp"
        android:layout_height="247dp"
        android:layout_marginBottom="468dp"
        android:src="@drawable/waves"
        app:layout_constraintBottom_toTopOf="@+id/done_iv"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="556dp"
        android:layout_height="149dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="40dp"
        android:src="@drawable/valueinapp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputLayout

        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="169dp"
        android:layout_height="72dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="30dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout2">

        <EditText
            android:id="@+id/age_info"
            android:layout_width="151dp"
            android:layout_height="49dp"
            android:ems="10"
            android:hint="@string/register_age"
            android:inputType="textPersonName"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="169dp"
        android:layout_height="72dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        app:boxStrokeColor="@color/value_green"
        app:hintTextColor="@color/value_green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout5">

        <EditText
            android:id="@+id/steps_goal"
            android:layout_width="151dp"
            android:layout_height="49dp"
            android:ems="10"
            android:hint="@string/register_steps_goal"
            android:inputType="textPersonName"
            android:minHeight="48dp"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    </com.google.android.material.textfield.TextInputLayout>

</androidx.constraintlayout.widget.ConstraintLayout>